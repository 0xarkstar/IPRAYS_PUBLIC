# ========================================
# Irys Canvas - Backend/Contract Environment Template
# ========================================

# Contract Deployment
PRIVATE_KEY=your_private_key_here

# Network Configuration  
IRYS_TESTNET_RPC=https://testnet-rpc.irys.xyz/v1/execution-rpc
IRYS_MAINNET_RPC=https://rpc.irys.xyz/v1/execution-rpc
IRYS_TESTNET_CHAIN_ID=1270

# Contract Settings
PIXEL_PRICE=1000000000000000

# Deployed Contract Addresses
CONTRACT_ADDRESS=your_contract_address_here
IMPLEMENTATION_ADDRESS=your_implementation_address_here

# Canvas Configuration
CANVAS_WIDTH=100
CANVAS_HEIGHT=100
EXPANSION_THRESHOLD=1000.0
MAX_CANVAS_SIZE=1000
INITIAL_PIXEL_PRICE=1.0

# API Keys (Optional)
ETHERSCAN_API_KEY=your_etherscan_api_key_here
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here

# Development Settings
REPORT_GAS=true
NODE_ENV=development

# Security Notice:
# - Generate a new private key for production deployment
# - Never commit real private keys to version control
# - Use environment-specific values for contract addresses